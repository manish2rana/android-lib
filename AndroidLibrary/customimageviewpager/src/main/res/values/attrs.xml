<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MaterialCustomVp">
        <attr name="civp_titleColor" format="color"/>
        <attr name="civp_titleSize" format="dimension"/>
        <attr name="civp_overlapCards" format="boolean"/>
        <attr name="civp_animateCards" format="boolean"/>

        <attr name="civp_cardElevationEnlarged" format="integer"/>
        <attr name="civp_cardElevationReduced" format="integer"/>

        <attr name="civp_lineSpacing" format="dimension"/>

        <attr name="civp_paddingTop" format="dimension"/>
        <attr name="civp_paddingBottom" format="dimension"/>
        <attr name="civp_paddingLeft" format="dimension"/>
        <attr name="civp_paddingRight" format="dimension"/>

        <attr name="civp_background" format="reference"/>
        <attr name="civp_backgroundOverlay" format="float"/>
        <attr name="civp_backgroundOverlayColor" format="color"/>

    </declare-styleable>


